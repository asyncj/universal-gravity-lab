<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.universalgravitylab.clientapp.controller.SimulationController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <ToolBar layoutX="3.0" layoutY="459.0" prefHeight="40.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0">
            <items>
                <Button defaultButton="true" mnemonicParsing="false" onAction="#onPlayPause" text="Play/Pause" />
                <Button cancelButton="true" mnemonicParsing="false" onAction="#onStopSimulation" text="Stop" />
                <Button mnemonicParsing="false" onAction="#onChangeLeft" text="&lt;&lt;" />
                <Slider fx:id="progressBar" prefHeight="16.0" prefWidth="367.0" />
                <Button mnemonicParsing="false" onAction="#onChangeRight" text="&gt;&gt;" />
            </items>
        </ToolBar>
        <Canvas fx:id="canvas" height="395.0" layoutX="14.0" layoutY="14.0" opacity="0.96" width="601.0" AnchorPane.bottomAnchor="65.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0" />
    </children>
</AnchorPane>
